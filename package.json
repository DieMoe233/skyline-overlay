{
  "engines": {
    "node": ">=14",
    "pnpm": ">=6"
  },
  "private": true,
  "type": "module",
  "name": "skyline-overlay",
  "version": "3.7.4",
  "description": "A modern customizable horizon FFXIV miniparse overlay.",
  "license": "Apache-2.0",
  "author": "DSRKafuU <dsrkafuu@outlook.com> (https://dsrkafuu.net)",
  "scripts": {
    "eslint": "eslint --ext \".jsx,.js,.tsx,.ts\" \"./src\"",
    "prettier": "prettier --write .",
    "typecheck": "tsc",
    "bundle": "vite build",
    "serve": "vite",
    "script:genIconIdx": "node \"./scripts/genIconIdx.mjs\"",
    "script:genMeta": "node \"./scripts/genMeta.mjs\"",
    "script:modSource": "node \"./scripts/modSource.mjs\"",
    "script:removeTrash": "node \"./scripts/removeTrash.mjs\"",
    "build": "cross-env NODE_ENV=production run-s script:genIconIdx script:genMeta eslint typecheck bundle script:removeTrash",
    "dev": "cross-env NODE_ENV=development run-s script:genIconIdx script:genMeta serve",
    "start": "serve"
  },
  "dependencies": {
    "@reduxjs/toolkit": "~1.8.6",
    "clsx": "~1.2.1",
    "ffxiv-overlay-api": "~4.5.0",
    "normalize.css": "~8.0.1",
    "react": "~18.2.0",
    "react-colorful": "~5.6.1",
    "react-dom": "~18.2.0",
    "react-redux": "~8.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@types/node": "^18.11.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitejs/plugin-react": "^2.1.0",
    "chalk": "^5.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rollup-plugin-visualizer": "^5.8.3",
    "sass": "^1.55.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vite-plugin-html-env": "^1.2.7",
    "vite-plugin-pwa": "^0.13.1",
    "vite-plugin-svgr": "^2.2.2",
    "workbox-window": "^6.5.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup"
      ]
    }
  }
}
