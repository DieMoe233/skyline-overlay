{
  "private": true,
  "type": "module",
  "name": "skyline-overlay",
  "version": "3.0.0-rc.1",
  "description": "A modern customizable horizon FFXIV miniparse overlay.",
  "license": "Apache-2.0",
  "author": "DSRKafuU <dsrkafuu@outlook.com> (https://dsrkafuu.net)",
  "scripts": {
    "eslint": "eslint --ext \".jsx,.js,.tsx,.ts\" \"./src\"",
    "prettier": "prettier --write .",
    "typecheck": "tsc",
    "bundle": "vite build",
    "serve": "vite",
    "script:genIconIdx": "node \"./scripts/genIconIdx.js\"",
    "script:genMeta": "node \"./scripts/genMeta.js\"",
    "script:removeSVG": "node \"./scripts/removeSVG.js\"",
    "build": "cross-env NODE_ENV=production run-s script:genIconIdx script:genMeta eslint typecheck bundle script:removeSVG",
    "dev": "cross-env NODE_ENV=development run-s script:genIconIdx script:genMeta serve",
    "start": "serve"
  },
  "dependencies": {
    "@sentry/react": "~6.16.0",
    "@sentry/tracing": "~6.16.0",
    "classnames": "~2.3.1",
    "ffxiv-overlay-api": "~4.1.3",
    "mobx": "~6.3.8",
    "mobx-react-lite": "~3.2.2",
    "normalize.css": "~8.0.1",
    "react": "~17.0.2",
    "react-dom": "~17.0.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "~7.16.0",
    "@svgr/rollup": "~6.1.1",
    "@types/node": "~16.11.12",
    "@types/react": "~17.0.37",
    "@types/react-dom": "~17.0.11",
    "@typescript-eslint/eslint-plugin": "~5.6.0",
    "@typescript-eslint/parser": "~5.6.0",
    "@vitejs/plugin-react": "~1.1.1",
    "chalk": "~5.0.0",
    "cross-env": "~7.0.3",
    "eslint": "~8.4.1",
    "eslint-plugin-react": "~7.27.1",
    "eslint-plugin-react-hooks": "~4.3.0",
    "glob": "~7.2.0",
    "npm-run-all": "~4.1.5",
    "prettier": "~2.5.1",
    "rollup-plugin-visualizer": "~5.5.2",
    "sass": "~1.44.0",
    "serve": "~13.0.2",
    "typescript": "~4.5.2",
    "vite": "~2.7.1",
    "vite-plugin-html-env": "~1.0.3"
  }
}
