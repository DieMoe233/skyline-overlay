{
  "private": true,
  "name": "skyline-overlay",
  "version": "2.2.1",
  "description": "A horizon FFXIV miniparse overlay built with React & Vite.",
  "license": "Apache-2.0",
  "author": "DSRKafuU <dsrkafuu@outlook.com> (https://dsrkafuu.net)",
  "scripts": {
    "svgr": "@svgr/cli -d \"./src/assets/svgs\" --ext \"tsx\" --svg-props \"className=icon\" --icon --typescript --ignore-existing \"./src/assets/svgs\"",
    "lint": "eslint --ext \".jsx,.js,.tsx,.ts\" \"./src\"",
    "compile": "vite build",
    "cleanup": "node \"./scripts/cleanup.js\"",
    "build": "cross-env NODE_ENV=production concurrently -m 1 npm:lint npm:compile npm:cleanup",
    "dev": "cross-env NODE_ENV=development vite"
  },
  "dependencies": {
    "classnames": "~2.3.1",
    "ffxiv-overlay-api": "4.0.0-beta.2",
    "i18next": "~21.5.2",
    "lodash": "~4.17.21",
    "mobx": "~6.3.7",
    "mobx-react-lite": "~3.2.2",
    "normalize.css": "~8.0.1",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-i18next": "~11.14.2",
    "react-transition-group": "~4.4.2"
  },
  "devDependencies": {
    "@svgr/cli": "~5.5.0",
    "@types/lodash": "~4.14.177",
    "@types/node": "~16.11.9",
    "@types/react": "~17.0.35",
    "@types/react-dom": "~17.0.11",
    "@typescript-eslint/eslint-plugin": "~5.4.0",
    "@typescript-eslint/parser": "~5.4.0",
    "@vitejs/plugin-react-refresh": "~1.3.6",
    "chalk": "~4.1.2",
    "concurrently": "~6.4.0",
    "cross-env": "~7.0.3",
    "eslint": "~8.2.0",
    "eslint-plugin-react": "~7.27.1",
    "eslint-plugin-react-hooks": "~4.3.0",
    "glob": "~7.2.0",
    "prettier": "~2.4.1",
    "sass": "~1.43.4",
    "typescript": "~4.5.2",
    "vite": "~2.6.14"
  }
}
